<Window x:Class="ClientWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="600">
    <Grid>
        <Button x:Name="btnConnect" Click="btnConnect_Click" Content="Connect" HorizontalAlignment="Left" Margin="322,26,0,0" VerticalAlignment="Top" Width="113" RenderTransformOrigin="-0.833,-0.761" Height="38"/>
        <TextBox x:Name="tbUserName" HorizontalAlignment="Left" Height="23" Margin="96,35,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="206" RenderTransformOrigin="0.954,2.848"/>
        <Label x:Name="lblUserName" Content="UserName:" HorizontalAlignment="Left" Margin="17,32,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="lbChat" HorizontalAlignment="Left" Height="223" Margin="17,92,0,0" VerticalAlignment="Top" Width="442"/>
        <TextBox x:Name="tbInput" HorizontalAlignment="Left" Height="75" Margin="17,329,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="442"/>
        <ListBox x:Name="lbUsers" HorizontalAlignment="Left" Height="312" Margin="481,92,0,0" VerticalAlignment="Top" Width="101"/>
        <Label x:Name="lblUsers" Content="Users:" HorizontalAlignment="Left" Margin="492,61,0,0" VerticalAlignment="Top" Width="65"/>
    </Grid>
</Window>
    
